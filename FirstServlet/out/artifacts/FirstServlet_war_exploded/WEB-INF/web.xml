<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>first</servlet-name>
        <servlet-class>com.mark.FirstServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>first</servlet-name>
        <url-pattern>/hi</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>HyperLink</servlet-name>
        <servlet-class>com.mark.HyperLink</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HyperLink</servlet-name>
        <url-pattern>/Hyper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Student</servlet-name>
        <servlet-class>com.mark.StudentInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Student</servlet-name>
        <url-pattern>/student</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Add</servlet-name>
        <servlet-class>com.mark.AddOps</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RequestMethod</servlet-name>
        <servlet-class>com.mark.RquestMethod</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RequestMethod</servlet-name>
        <url-pattern>/req</url-pattern>
    </servlet-mapping>

</web-app>